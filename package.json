{
  "name": "kitsu-season-trends",
  "version": "1.0.0",
  "description": "Kitsu seasonal anime trends",
  "scripts": {
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.production.config.js --colors --progress",
    "build:rename": "node webpack.rename",
    "build": "yarn build:webpack && yarn build:rename",
    "start": "webpack-serve --config webpack.config.js --port 8888 --content dist",
    "lint": "eslint --ext js --ext jsx src",
    "limit": "size-limit",
    "cron": "node --experimental-modules cron"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wopian/kitsu-season-trends"
  },
  "author": "James Harris <wopian@wopian.me>",
  "license": "MIT",
  "homepage": "https://github.com/wopian/kitsu-season-trends#readme",
  "dependencies": {
    "classnames": "~2.2.5",
    "fromnow": "~3.0.0",
    "lodash-es": "~4.17.10",
    "prop-types": "~15.6.0",
    "react": "~16.4.0",
    "react-dom": "~16.4.0",
    "react-ga": "~2.5.0",
    "react-icons": "~3.1.0",
    "react-lazy-load": "~3.0.13",
    "react-router-dom": "~4.3.0",
    "recharts": "1.3.0"
  },
  "devDependencies": {
    "@babel/cli": "~7.1.0",
    "@babel/core": "~7.1.0",
    "@babel/node": "~7.0.0",
    "@babel/plugin-proposal-class-properties": "~7.1.0",
    "@babel/plugin-syntax-decorators": "~7.1.0",
    "@babel/plugin-transform-runtime": "~7.1.0",
    "@babel/preset-env": "~7.1.0",
    "@babel/preset-react": "~7.0.0",
    "@babel/runtime": "~7.1.0",
    "autoprefixer": "~9.1.0",
    "babel-loader": "~8.0.0",
    "chalk": "~2.4.0",
    "clean-webpack-plugin": "~0.1.19",
    "client-oauth2": "~4.2.1",
    "connect-history-api-fallback": "~1.5.0",
    "copy-webpack-plugin": "~4.5.0",
    "cross-env": "~5.2.0",
    "css-loader": "~1.0.0",
    "eslint": "~5.6.0",
    "eslint-config-wopian-react": "~1.4.0",
    "html-webpack-plugin": "~3.2.0",
    "json5": "~2.1.0",
    "kitsu": "~6.4.0",
    "koa-connect": "~2.0.1",
    "mini-css-extract-plugin": "~0.4.1",
    "msgpack-lite": "~0.1.26",
    "optimize-css-assets-webpack-plugin": "~5.0.0",
    "react-hot-loader": "~4.3.0",
    "resource-hints-webpack-plugin": "~0.0.2",
    "size-limit": "~0.20.0",
    "style-loader": "~0.23.0",
    "stylus": "~0.54.5",
    "stylus-loader": "~3.0.2",
    "sw-precache-webpack-plugin": "~0.11.5",
    "terser-webpack-plugin": "~1.1.0",
    "url-loader": "~1.1.0",
    "webpack": "~4.19.0",
    "webpack-bundle-size-analyzer": "~2.7.0",
    "webpack-cli": "~3.1.0",
    "webpack-dashboard": "~2.0.0",
    "webpack-pwa-manifest": "~3.7.0",
    "webpack-serve": "~2.0.0",
    "weighted-mean": "~1.0.2"
  },
  "renovate": {
    "extends": [
      "@wopian"
    ]
  },
  "size-limit": [
    {
      "path": "dist/main*.js",
      "limit": "155.80 KB"
    },
    {
      "path": "dist/main*.css",
      "limit": "4.11 KB"
    }
  ]
