{
  "name": "kitsu-season-trends",
  "version": "1.0.0",
  "description": "Kitsu seasonal anime trends",
  "scripts": {
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.production.config.js --colors --progress",
    "build:webpack-ci": "cross-env NODE_ENV=production webpack --config webpack.production.config.js",
    "build:rename": "node webpack.rename",
    "build": "yarn build:webpack && yarn build:rename",
    "build:ci": "yarn build:webpack-ci && yarn build:rename",
    "start": "webpack-dev-server --content-base dist --history-api-fallback --hot",
    "lint": "eslint --ext js --ext jsx src",
    "limit": "size-limit",
    "cron": "node cron/index.mjs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wopian/kitsu-season-trends"
  },
  "author": "James Harris <wopian@wopian.me>",
  "license": "MIT",
  "homepage": "https://github.com/wopian/kitsu-season-trends#readme",
  "dependencies": {
    "@loadable/component": "~5.13.0",
    "classnames": "~2.2.5",
    "fromnow": "~3.0.0",
    "lodash": "~4.17.14",
    "prop-types": "~15.7.0",
    "react": "~16.13.0",
    "react-dom": "~16.13.0",
    "react-ga": "~3.1.0",
    "react-icons": "~3.10.0",
    "react-lazy-load": "~3.1.0",
    "react-router-dom": "~5.2.0",
    "recharts": "~1.8.0"
  },
  "devDependencies": {
    "@babel/cli": "~7.10.0",
    "@babel/core": "~7.10.0",
    "@babel/node": "~7.10.0",
    "@babel/plugin-proposal-class-properties": "~7.10.0",
    "@babel/plugin-syntax-decorators": "~7.10.0",
    "@babel/plugin-syntax-dynamic-import": "~7.8.3",
    "@babel/plugin-transform-runtime": "~7.10.0",
    "@babel/preset-env": "~7.10.0",
    "@babel/preset-react": "~7.10.0",
    "@babel/runtime": "~7.10.0",
    "@loadable/babel-plugin": "^5.12.0",
    "autoprefixer": "~9.8.0",
    "babel-loader": "~8.1.0",
    "chalk": "~4.0.0",
    "clean-webpack-plugin": "~3.0.0",
    "client-oauth2": "~4.3.0",
    "connect-history-api-fallback": "~1.6.0",
    "copy-webpack-plugin": "~6.0.0",
    "cross-env": "~7.0.0",
    "css-loader": "~3.6.0",
    "eslint": "~7.4.0",
    "eslint-config-wopian-react": "~2.0.1",
    "html-webpack-plugin": "~4.3.0",
    "json5": "~2.1.0",
    "kitsu": "~9.1.8",
    "koa-connect": "~2.0.1",
    "mini-css-extract-plugin": "~0.9.0",
    "msgpack-lite": "~0.1.26",
    "optimize-css-assets-webpack-plugin": "~5.0.0",
    "react-hot-loader": "~4.12.0",
    "resource-hints-webpack-plugin": "~0.0.2",
    "size-limit": "~1.3.0",
    "style-loader": "~1.2.0",
    "stylus": "~0.54.5",
    "stylus-loader": "~3.0.2",
    "sw-precache-webpack-plugin": "~1.0.0",
    "terser-webpack-plugin": "~3.0.0",
    "url-loader": "~4.1.0",
    "webpack": "~4.43.0",
    "webpack-cli": "~3.3.0",
    "webpack-dashboard": "~3.2.0",
    "webpack-dev-server": "~3.11.0",
    "webpack-pwa-manifest": "~4.2.0",
    "weighted-mean": "~1.0.2"
  },
  "renovate": {
    "extends": [
      "@wopian"
    ]
  },
  "size-limit": [
    {
      "path": "dist/main*.js",
      "limit": "155.80 KB"
    },
    {
      "path": "dist/main*.css",
      "limit": "4.11 KB"
    }
  ]
